<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SecondFragment">

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="50dp"
        android:layout_height="2dp"
        android:layout_marginTop="32dp"
        android:background="#000000"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/player5"></LinearLayout>

    <TextView
        android:id="@+id/player5"
        android:layout_width="115dp"
        android:layout_height="0dp"
        android:layout_marginTop="45dp"
        android:gravity="center"
        android:text="None"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/player2"
        app:layout_constraintTop_toBottomOf="@+id/player2" />

    <TextView
        android:id="@+id/player3"
        android:layout_width="115dp"
        android:layout_height="0dp"
        android:layout_marginEnd="64dp"
        android:layout_marginRight="64dp"
        android:gravity="center"
        android:text="None"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/player1" />

    <TextView
        android:id="@+id/player7"
        android:layout_width="115dp"
        android:layout_height="0dp"
        android:layout_marginTop="45dp"
        android:gravity="center"
        android:text="None"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/player4"
        app:layout_constraintTop_toBottomOf="@+id/player4" />

    <TextView
        android:id="@+id/player4"
        android:layout_width="115dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="None"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/player3"
        app:layout_constraintTop_toBottomOf="@+id/player3" />

    <TextView
        android:id="@+id/player2"
        android:layout_width="115dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="None"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/player1"
        app:layout_constraintTop_toBottomOf="@+id/player1" />

    <TextView
        android:id="@+id/player1"
        android:layout_width="115dp"
        android:layout_height="0dp"
        android:layout_marginStart="64dp"
        android:layout_marginLeft="64dp"
        android:layout_marginTop="32dp"
        android:gravity="center"
        android:text="None"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/player8"
        android:layout_width="115dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="None"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/player7"
        app:layout_constraintTop_toBottomOf="@+id/player7" />

    <TextView
        android:id="@+id/player6"
        android:layout_width="115dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="None"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/player5"
        app:layout_constraintTop_toBottomOf="@+id/player5" />

    <Button
        android:id="@+id/button_second"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:text="Generate Matchup"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="50dp"
        android:layout_height="2dp"
        android:layout_marginTop="32dp"
        android:background="#000000"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/player1"></LinearLayout>

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group"
        android:layout_width="2dp"
        android:layout_height="2dp"
        tools:layout_editor_absoluteX="188dp"
        tools:layout_editor_absoluteY="422dp" />

    <EditText
        android:id="@+id/addPlayers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="64dp"
        android:ems="10"
        android:inputType="text"
        android:text="Add Players"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/player6" />

    <Button
        android:id="@+id/returningPlayersButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:text="Submit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/returningPlayers" />

    <Button
        android:id="@+id/removePlayersButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:text="Submit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/removePlayers" />

    <EditText
        android:id="@+id/returningPlayers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:inputType="text"
        android:text="Re-Admit Players"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/removePlayers" />

    <Button
        android:id="@+id/addPlayersButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:text="Submit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/addPlayers" />

    <EditText
        android:id="@+id/removePlayers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:inputType="text"
        android:text="Remove Players"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addPlayers" />

    <Button
        android:id="@+id/win1"
        android:layout_width="62dp"
        android:layout_height="66dp"
        android:text="Win"
        app:layout_constraintEnd_toStartOf="@+id/player1"
        app:layout_constraintTop_toTopOf="@+id/player1" />

    <Button
        android:id="@+id/win4"
        android:layout_width="62dp"
        android:layout_height="66dp"
        android:text="Win"
        app:layout_constraintStart_toEndOf="@+id/player7"
        app:layout_constraintTop_toTopOf="@+id/player7" />

    <Button
        android:id="@+id/win3"
        android:layout_width="62dp"
        android:layout_height="66dp"
        android:text="Win"
        app:layout_constraintEnd_toStartOf="@+id/player5"
        app:layout_constraintTop_toTopOf="@+id/player5" />

    <Button
        android:id="@+id/win2"
        android:layout_width="62dp"
        android:layout_height="66dp"
        android:text="Win"
        app:layout_constraintStart_toEndOf="@+id/player3"
        app:layout_constraintTop_toTopOf="@+id/player3" />

    <TextView
        android:id="@+id/winText"
        android:layout_width="106dp"
        android:layout_height="41dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="48dp"
        android:text="Winners:"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/returningPlayers" />

    <TextView
        android:id="@+id/winTextOutput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="6dp"
        android:text="None"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/winText" />

    <Button
        android:id="@+id/scoreboard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:text="View Scoreboard"
        app:layout_constraintBottom_toBottomOf="@+id/winText"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>